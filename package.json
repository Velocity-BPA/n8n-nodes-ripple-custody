{
  "name": "n8n-nodes-ripple-custody",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Ripple Custody (Metaco Harmonize) institutional digital asset custody platform, providing 23 resources and 250+ operations for vault management, multi-chain transactions, tokenization, staking, DeFi, cold storage, and regulatory compliance.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-ripple-custody.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-ripple-custody#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-ripple-custody/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "ripple",
    "custody",
    "metaco",
    "harmonize",
    "digital-assets",
    "cryptocurrency",
    "blockchain",
    "institutional",
    "vault",
    "wallet",
    "tokenization",
    "staking",
    "defi",
    "cold-storage",
    "mpc",
    "hsm",
    "compliance",
    "kyt"
  ],
  "main": "dist/nodes/RippleCustody/RippleCustody.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RippleCustodyApi.credentials.js",
      "dist/credentials/RippleCustodyHsm.credentials.js",
      "dist/credentials/RippleCustodyMpc.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RippleCustody/RippleCustody.node.js",
      "dist/nodes/RippleCustody/RippleCustodyTrigger.node.js"
    ]
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && gulp",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
