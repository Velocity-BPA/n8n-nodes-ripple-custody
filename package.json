  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "ethers": "^6.9.0",
    "@solana/web3.js": "^1.87.0",
    "jose": "^5.1.0",
    "uuid": "^9.0.0"
